{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
     "moduleResolution": "node",
    "paths": {
      "*": ["tsconfig/*"],
       "@imports/*": ["src/imports/*"]

    }
  },
  "include": ["declarations.d.ts", "src/**/*.ts","src/**/*.tsx",
  "index.tsx",
  "/App/**/*.ts",
  "App.tsx",
  "app.d.ts",
  "App/**/*.tsx",]
}
